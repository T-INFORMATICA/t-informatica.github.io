---
layout: null
---

{
{%- for coursedata in site.data.courses %}
    {% for subject in coursedata.subjects %}
        {%- assign siteDefs = false -%}
    "{{subject.name}}": [
            {%- for chapter in subject.chapters -%}
                {% comment %}{%- unless forloop.first or siteDefs == false %},{% endunless -%}{% endcomment %}
                {%- assign courseDefs = false -%}
                {%- for course in site.courses -%}
                    {%- if course.url == chapter.link %}
                        {%- for definition in course.definitions %}
                            {%- if courseDefs == true or siteDefs == true %}, {% endif -%}
                            {% comment %}{%- unless forloop.first or courseDefs == false or siteDefs == false %},{% endunless -%}{% endcomment %}
                            {%- assign siteDefs = true -%}
                            {%- assign courseDefs = true %}
        {
            "term": "{{definition.name}}",
            "definition": "{{definition.definition}}"
        }
                        {%- endfor -%}
                    {%- endif -%}
                {%- endfor -%}
            {%- endfor %}
    ]
            {%- unless forloop.last %},{% endunless -%}
        {%- endfor -%}
    {%- unless forloop.last %},{% endunless -%}
{% endfor %}
}